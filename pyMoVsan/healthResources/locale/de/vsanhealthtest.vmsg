com.vmware.vsan.health.test.data.vmHealth.testName = vSAN-VM-Integrität
com.vmware.vsan.health.test.data.vmHealth.testResult.label = Übersicht VM-Integrität
com.vmware.vsan.health.test.data.vmHealth.testResult.columnHealthVM = Integritätsstatus
com.vmware.vsan.health.test.data.vmHealth.testResult.columnNumber = Anzahl
