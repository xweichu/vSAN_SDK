# Remain unchanged
#
vsan.health.test.hciskip.label = "vSAN-Integritätsalarme werden unterdrückt"
vsan.health.test.hciskip.summary = "vSAN-Integritätsalarme werden unterdrückt, bis der Cluster vollständig konfiguriert wird oder dieser Schnellstart-Workflow übersprungen wird."

vsan.health.test.vsanhardwarecert.label = "Probleme bei der vSAN-Hardwarekompatibilität"
vsan.health.test.vsanhardwarecert.summary = "Dieser Alarm wird ausgelöst, wenn im Cluster Probleme mit der Hardwarekompatibilität festgestellt werden. Detailliertere Problemstellungen finden Sie in der Benutzeroberfläche der vSAN-Systemüberwachung."

vsan.health.test.cloudhealth.vsanenablesupportinsight.label = "vSAN Support Insight"
vsan.health.test.cloudhealth.vsanenablesupportinsight.summary = "Standardalarm zur Überwachung des Status von vSAN Support Insight"
vsan.health.test.cloudhealth.advisor.label = "vSAN-Integritätsalarm ‚Advisor‘"
vsan.health.test.cloudhealth.advisor.summary = "Standardalarm zur Überwachung von Änderungen für den Integritätstest ‚Advisor‘"

vsan.health.test.diagnostics.pnic.label = "Hohe pNic-Fehlerrate erkannt. Details finden Sie in der vSAN-Leistungsansicht des Hosts"
vsan.health.test.diagnostics.pnic.summary = "Standardalarm zum Überwachen von ,pNic-Fehlerrate‘"

vsan.health.test.diagnostics.rdt.label = "Hoher RDT-Netzfehler erkannt. Details finden Sie in der vSAN-Leistungsansicht des Hosts"
vsan.health.test.diagnostics.rdt.summary = "Standardalarm zur Überwachung von „RDT-Netzfehler“"

