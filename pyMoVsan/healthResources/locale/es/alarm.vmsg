# Remain unchanged
#
vsan.health.test.hciskip.label = "Se suprimieron las alarmas de estado de vSAN"
vsan.health.test.hciskip.summary = "Las alarmas de estado de vSAN se suprimen hasta que el clúster se configure por completo o hasta que se omita este flujo de trabajo de inicio rápido."

vsan.health.test.vsanhardwarecert.label = "Problemas de compatibilidad de hardware de vSAN"
vsan.health.test.vsanhardwarecert.summary = "Esta alarma se activará si se encuentran algunos problemas de compatibilidad de hardware en el clúster. Consulte la interfaz de supervisión de estado de vSAN para ver problemas más detallados."

vsan.health.test.cloudhealth.vsanenablesupportinsight.label = "vSAN Support Insight"
vsan.health.test.cloudhealth.vsanenablesupportinsight.summary = "Alarma predeterminada para supervisar el estado de vSAN Support Insight"
vsan.health.test.cloudhealth.advisor.label = "Alarma de estado de vSAN 'Advisor'"
vsan.health.test.cloudhealth.advisor.summary = "Alarma predeterminada para supervisar cambios en la prueba de estado 'Advisor'"

vsan.health.test.diagnostics.pnic.label = "Se detectó una tasa de errores alta de pNIC. Compruebe la vista de rendimiento de vSAN del host para obtener más información"
vsan.health.test.diagnostics.pnic.summary = "Alarma predeterminada para supervisar 'Tasa de errores de pNIC'"

vsan.health.test.diagnostics.rdt.label = "Se detectó un error de red de RDT. Compruebe la vista de rendimiento de vSAN del host para obtener más información"
vsan.health.test.diagnostics.rdt.summary = "Alarma predeterminada para supervisar el 'Error de red de RDT'"

