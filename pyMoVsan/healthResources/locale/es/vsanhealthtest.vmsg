com.vmware.vsan.health.test.network.multicastDeepDive.testResult.packetLabel = Paquetes
com.vmware.vsan.health.test.network.multicastDeepDive.testResult.columnTimestamp = Marca de tiempo
com.vmware.vsan.health.test.network.multicastDeepDive.testResult.columnMsgType = TipoMens
